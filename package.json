{
  "name": "data-catalog-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/enzyme": "^3.9.0",
    "bootstrap": "^4.3.1",
    "connected-react-router": "^6.3.2",
    "font-awesome": "^4.7.0",
    "json-server": "^0.14.2",
    "npm-run-all": "^4.1.5",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-fontawesome": "^1.6.1",
    "react-hot-loader": "^4.8.0",
    "react-redux": "^6.0.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "3.1.0",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^0.16.2",
    "redux-saga-test-plan": "^3.7.0",
    "typesafe-actions": "^3.2.1"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "start-with-stub": "npm-run-all -p start stub-server",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "stub-server": "json-server --watch ./src/stub-api/db.json --port 8080 --routes ./src/stub-api/routes.json --delay 1000",
    "coverage": "npm test a -- --coverage --collectCoverageFrom='src/!(_poc*)**/*.{js,jsx,ts,tsx}'",
    "formatcode": "./node_modules/.bin/prettier --single-quote --trailing-comma none --ignore-path \"**/.history\" --print-width 90 --write \"{src,__{tests,mocks}__}/**/*.{js,jsx,ts,tsx,json}\""
  },
  "devDependencies": {
    "@types/history": "^4.7.2",
    "@types/jest": "^24.0.9",
    "@types/lodash": "^4.14.122",
    "@types/moment": "^2.13.0",
    "@types/node": "^11.9.5",
    "@types/qs": "^6.5.2",
    "@types/react": "^16.8.5",
    "@types/react-dom": "^16.8.2",
    "@types/react-fontawesome": "^1.6.4",
    "@types/react-redux": "^7.0.3",
    "@types/react-router": "^4.4.4",
    "@types/react-router-dom": "^4.3.1",
    "prettier": "1.16.4",
    "typescript": "^3.3.3333"
  }
}
